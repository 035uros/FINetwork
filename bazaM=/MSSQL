/****** Object:  StoredProcedure [dbo].[app_Login]    Script Date: 14-Jan-23 8:34:08 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROC [dbo].[app_Login](@korisnicko_ime VARCHAR(100), @lozinka VARCHAR(50))
AS 
BEGIN
	SELECT * from korisnik WHERE @korisnicko_ime=@korisnicko_ime AND lozinka=@lozinka;
END
GO
/****** Object:  StoredProcedure [dbo].[app_Reg]    Script Date: 14-Jan-23 8:34:08 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROC [dbo].[app_Reg]( @korisnicko_ime VARCHAR(255), @ime VARCHAR(100), @prezime VARCHAR(100), @email VARCHAR(100), @lozinka VARCHAR(30))
AS 
BEGIN
	INSERT INTO korisnik (id_tipa, korisnicko_ime, ime, prezime, email, lozinka, verifikovan) VALUES (2, @korisnicko_ime, @ime, @prezime, @email, @lozinka, 0);
END
GO
USE [master]
GO
ALTER DATABASE [finetwork] SET  READ_WRITE 
GO
